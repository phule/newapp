<h1>Listing state_signups</h1>

<table>
  <tr>
    <th>Email</th>
    <th>Active</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @state_signups.each do |state_signup| %>
  <tr>
    <td><%= state_signup.email %></td>
    <td><%= state_signup.active %></td>
    <td><%= link_to 'Show', state_signup %></td>
    <td><%= link_to 'Edit', edit_state_signup_path(state_signup) %></td>
    <td><%= link_to 'Destroy', state_signup, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New State signup', new_state_signup_path %>
